<Window x:Class="myFire_Lists"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="myFire_Lists" Height="369" Width="527" Icon="/MyFire;component/Resources/Retro-Fire-Ball.ico" WindowStartupLocation="CenterScreen">
    <Grid>
        <ListBox ScrollViewer.CanContentScroll="False" ScrollViewer.HorizontalScrollBarVisibility="Disabled" HorizontalContentAlignment="Stretch" Margin="0,41,0,0" Name="ListList" Style="{DynamicResource myFireListBoxStyle}" ItemTemplate="{DynamicResource TwitterListTemplete}"/>
        <Button Content="New List" Height="23" HorizontalAlignment="Right" Margin="0,9,12,0" Name="Button1" VerticalAlignment="Top" Width="75" />
        <TextBlock HorizontalAlignment="Left" Margin="12,13,0,0" Name="TextBlock1" Text="Manage your Lists" VerticalAlignment="Top" Width="315" />
    </Grid>
</Window>
